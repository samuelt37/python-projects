8.11 OEHydrogenMode 
 
   
namespace OEHydrogenMode
{
    const unsigned int Undefined         = 0;
    const unsigned int ImplicitHydrogens = 0;
    const unsigned int ExplicitPolar     = 0;
    const unsigned int ExplicitHydrogens = 0;
    const unsigned int Protomer          = 0;
}
    

  
          
     
        
