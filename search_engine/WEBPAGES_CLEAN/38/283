template
class pair {
  public:
    pair(){}
    pair(const F&amp; f,const S&amp; s) : first(f), second(s) {/*first = f; second = s; std::cout &amp; rhs) const {return first == rhs.first &amp;&amp; second == rhs.second;}
      bool operator != (const pair&amp; rhs) const {return !(*this==rhs);}

      template
      friend std::ostream&amp; operator &amp; p);
};

template
pair make_pair(F f, S s){return pair(f,s);}

