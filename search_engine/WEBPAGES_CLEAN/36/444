--Reagent (Level 2) Annotation Table
CREATE TABLE reagent_annotation
(
    reagent_annotation_id BIGSERIAL PRIMARY KEY,
    annotation_value TEXT NOT NULL,
    annotation_type_id INTEGER REFERENCES annotation_type(annotation_type_id),
    reagent_id INTEGER REFERENCES reagent(reagent_id)
);

 
