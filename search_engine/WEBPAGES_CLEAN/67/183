8.2 OECounts 
 
   
namespace OECounts
{
  const unsigned int Undefined  = 0;
  const unsigned int Atoms      = 1;
  const unsigned int Bonds      = 2;
  const unsigned int Conformers = 3;
  const unsigned int Poses      = 4;
}
    

  
          
     
        
