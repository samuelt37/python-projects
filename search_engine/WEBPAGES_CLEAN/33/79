23.3.1 OEReadMol2File 
 

 
    bool MyReadMol2Molecule(oemolistream &amp;ifs, OEMolBase &amp;mol)
{
  mol.Clear();
  if (OEReadMol2File(ifs,mol))
  {
    OEFindRingAtomsAndBonds(mol);
    OEAssignAromaticFlags(mol);
    return true;
  }
  mol.Clear();
  return false;
}
    

  
          
     
        
